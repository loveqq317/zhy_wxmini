<view class="tab_wrap">
    <view class="tab_title">
       <view class="title_item  {{item.isActive?'active':''}}"
             wx:for="{{tabs}}" wx:key="id" bindtap="handleTab" data-index="{{index}}">
           {{item.name}}
       </view>
    </view>

    <view class="tab_content">
        <slot></slot>

    </view>





</view>
